{
  "name": "dungeon-scribe-ai",
  "version": "1.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "emulators": "firebase emulators:start",
    "emulators:seed": "firebase emulators:start --import=./firebase-seed",
    "emulators:export": "firebase emulators:export ./firebase-seed",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@reduxjs/toolkit": "^2.11.2",
    "expo": "~55.0.3",
    "expo-apple-authentication": "~55.0.8",
    "expo-auth-session": "~55.0.6",
    "expo-constants": "~55.0.7",
    "expo-crypto": "~55.0.8",
    "expo-font": "~55.0.4",
    "expo-linear-gradient": "~55.0.8",
    "expo-linking": "~55.0.7",
    "expo-router": "~55.0.2",
    "expo-splash-screen": "~55.0.9",
    "expo-status-bar": "~55.0.4",
    "expo-web-browser": "~55.0.9",
    "firebase": "^12.9.0",
    "nativewind": "^4.2.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-web": "~0.20.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^9.39.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
